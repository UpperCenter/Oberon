{
	"private": true,
	"scripts": {
		"dev": "npm run development",
		"development": "mix",
		"watch": "mix watch",
		"watch-poll": "mix watch -- --watch-options-poll=1000",
		"hot": "mix watch --hot",
		"prod": "npm run blade && npm run prettier && npm run production",
		"production": "mix --production",
		"prettier": "./node_modules/.bin/prettier --write '**/*.php'",
		"blade": "./node_modules/.bin/blade-formatter --progress --write '**/*.blade.php'"
	},
	"devDependencies": {
		"@prettier/plugin-php": "^0.16.2",
		"@tailwindcss/forms": "^0.2.1",
		"@tailwindcss/typography": "^0.3.0",
		"alpinejs": "^2.7.3",
		"axios": "^0.21",
		"blade-formatter": "^1.11.5",
		"laravel-mix": "^6.0.18",
		"lodash": "^4.17.19",
		"postcss": "^8.2.13",
		"postcss-import": "^12.0.1",
		"prettier": "^2.2.1",
		"tailwindcss": "^2.1.2"
	},
	"dependencies": {}
}
